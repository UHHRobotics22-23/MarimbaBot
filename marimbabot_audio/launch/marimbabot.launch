<?xml version="1.0"?>
<launch>
    <node name="audio_from_lilypond" pkg="marimbabot_audio" type="audio_from_lilypond.py"/>
    <node name="audio_capture" pkg="audio_capture" type="audio_capture" output="screen">
        <param name="device" value="" />
        <param name="format" value="wave" />
        <param name="channels" value="1" />
        <param name="depth" value="16" />
        <param name="sample_rate" value="44100" />
    </node>
    <node name="onset_detector" pkg="marimbabot_audio" type="onset_detection.py" output="screen" />
    <node name="onset_midi" pkg="marimbabot_audio" type="note2midi.py" output="screen" />
    <node name="soundplay_node" pkg="sound_play" type="soundplay_node.py"/>
</launch>