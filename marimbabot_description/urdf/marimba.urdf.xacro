                <box size="${body_bottom_width} ${body_bottom_length} ${body_bottom_height}"/>
            </geometry>
            <material name="gray" />
        </visual>
        <collision>
            <origin xyz="${body_bottom_pos}" rpy="${body_bottom_rpy}" />
            <geometry>
                <box size="${body_bottom_width} ${body_bottom_length} ${body_bottom_height}"/>
