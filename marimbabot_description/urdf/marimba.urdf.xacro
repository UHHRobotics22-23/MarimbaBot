        <!-- TODO material -->
        <!--<material>Gazebo/Green</material>-->
        <mu1>50.0</mu1>
        <mu2>50.0</mu2>
        <kp>1000000.0</kp>
        <kd>1.0</kd>
    </gazebo>


    <!-- Bottom body -->

    <gazebo reference="body_bottom">
        <turnGravityOff>false</turnGravityOff>
        <selfCollide>true</selfCollide>
        <!-- TODO material -->
        <!-- <material>chordophones/OakWood</material> -->
