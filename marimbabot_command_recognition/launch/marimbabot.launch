<launch>
  <node name="audio_capture" pkg="audio_capture" type="audio_capture" output="screen">
    <param name="device" value="" />
    <param name="format" value="wave" />
    <param name="channels" value="1" />
    <param name="depth" value="2" />
    <param name="sample_rate" value="16000" />
  </node>
  <node name="command_audio2cache_node" pkg="marimbabot_command_recognition" type="audio2tmp.py"/>
  <node name="command_speech_recognition_node" pkg="marimbabot_command_recognition" type="speech_recognition.py"/>
</launch>
